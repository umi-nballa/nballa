<?xml version="1.0"?>
<!-- ********************************************************************************************************************
    ORGANIZATION TABLES
  ******************************************************************************************************************** -->
<extension
  xmlns="http://guidewire.com/datamodel"
  entityName="Organization">
  <column
    desc="The kanji name of the organization. Used only for Japanese and will be null otherwise."
    name="NameKanji"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="120"/>
  </column>
  <edgeForeignKey
    edgeTableName="MasterOrg"
    fkentity="Organization"
    name="MasterOrganization_Ext"
    nullok="true"/>
  <column
    desc="Agency Number"
    name="AgenyNumber_Ext"
    nullok="true"
    type="shorttext"/>
</extension>