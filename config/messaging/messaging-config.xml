<?xml version="1.0"?>
<!-- ==============================================================================
   Messaging Configuration
  ============================================================================== -->
<messaging-config
  xmlns="http://guidewire.com/messaging-config">
  <messaging>
    <destination
      id="65"
      initialretryinterval="500"
      internal="true"
      maxretries="5"
      name="Java.MessageDestination.Email.Name"
      transportplugin="emailMessageTransport"/>
    <destination
      disabled="true"
      id="68"
      initialretryinterval="100"
      internal="true"
      name="Java.MessageDestination.ConsoleMessageLogger"
      transportplugin="consoleTransport">
      <event
        name="\w*"/>
    </destination>
    <destination
      id="66"
      initialretryinterval="5000"
      internal="true"
      name="Java.MessageDestination.BillingSystem"
      requestplugin="BillingMessageRequest"
      transportplugin="BillingMessageTransport">
      <event
        name="(\w)*"/>
    </destination>
    <destination
      id="324"
      internal="true"
      name="Java.MessageDestination.DocumentStore"
      transportplugin="documentStoreTransport">
      <event
        name="DocumentStore"/>
    </destination>
    <destination
      id="67"
      internal="true"
      name="Java.MessageDestination.ContactMessageTransport"
      primaryentity="Contact"
      requestplugin="ContactMessageRequest"
      transportplugin="ContactMessageTransport">
      <event
        name="ContactAdded"/>
      <event
        name="ContactChanged"/>
      <event
        name="ContactRemoved"/>
    </destination>
    <destination
      disabled="true"
      id="69"
      internal="true"
      name="Java.MessageDestination.SolrMessageTransport.Policy.Name"
      pollinterval="1000"
      transportplugin="ISolrMessageTransportPlugin">
      <event
        name="PolicyPeriodAdded"/>
      <event
        name="PolicyPeriodChanged"/>
      <event
        name="PolicyPeriodRemoved"/>
      <event
        name="PreemptedPeriod"/>
      <event
        name="PolicyPeriodPurged"/>
      <event
        name="ContactChanged"/>
      <event
        name="ContactAdded"/>
      <event
        name="ContactRemoved"/>
      <event
        name="PolicyAddressChanged"/>
      <event
        name="PolicyPurged"/>
      <event
        name="JobPurged"/>
    </destination>
    <destination
      id="70"
      internal="true"
      name="Java.MessageDestination.HPXMessageTransport"
      transportplugin="HPXMessageTransport">
      <event
        name="CreatePeriod"/>
      <event
        name="ChangePeriod"/>
      <event
        name="RenewPeriod"/>
      <event
        name="RewritePeriod"/>
      <event
        name="SendNonRenewalDocuments"/>
      <event
        name="SendConsentToRate"/>
      <event
        name="SendCancellationNotices"/>
      <event
        name="SendDifferenceInConditions"/>
      <event
        name="SendAlarmCreditRemovalLetter"/>
      <event
        name="event"/>
    </destination>
    <destination
      id="1"
      name="OnBaseMessageTransport"
      transportplugin="OnBaseMessageTransport">
      <event
        name="DocumentChanged"/>
      <event
        name="IssuePeriod"/>
      <event
        name="ChangePeriod"/>
    </destination>
    <destination
      id="0"
      name="OnBaseMessageBrokerTransport"
      transportplugin="OnBaseMessageBrokerTransport">
      <event
        name="MessageComplete"/>
    </destination>
    <destination
      id="90"
      internal="true"
      name="LexisFirstMessage"
      transportplugin="LexisFirstMessageTransport">
      <event
        name="LexisFirstOnCreatePeriod"/>
      <event
        name="LexisFirstOnIssuePeriod"/>
      <event
        name="LexisFirstOnChangePeriod"/>
      <event
        name="LexisFirstOnReinstatePeriod"/>
      <event
        name="LexisFirstOnRewritePeriod"/>
      <event
        name="LexisFirstOnRenewPeriod"/>
    </destination>
    <destination
      id="23"
      name="IvansPersonalAuto"
      transportplugin="AcceleratorTransport">
      <event
        name="CancelPeriod"/>
      <event
        name="CreatePeriod"/>
      <event
        name="ChangePeriod"/>
      <event
        name="IssuePeriod"/>
      <event
        name="RenewPeriod"/>
      <event
        name="ReinstatePeriod"/>
      <event
        name="RewritePeriod"/>
    </destination>
    <destination
      id="31"
      name="UNAPolicyRefreshTransport"
      transportplugin="UNAPolicyRefreshTransport">
      <event
        name="CancelPeriod"/>
      <event
        name="CreatePeriod"/>
      <event
        name="ChangePeriod"/>
      <event
        name="IssuePeriod"/>
      <event
        name="RenewPeriod"/>
      <event
        name="ReinstatePeriod"/>
      <event
        name="RewritePeriod"/>
    </destination>
  </messaging>
</messaging-config>