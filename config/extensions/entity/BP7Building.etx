    desc="No Of Units"
    name="NoOfUnits_Ext"
    nullok="true"
    type="nonnegativeinteger"/>
  <column
    desc="Building Square Footage"
    name="BuildingSqFootage_Ext"
    nullok="true"
    type="nonnegativeinteger"/>
