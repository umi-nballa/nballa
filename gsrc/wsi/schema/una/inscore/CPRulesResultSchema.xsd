<xs:schema targetNamespace="http://cp.com/rules/client" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:rf="http://cp.com/rules/client" elementFormDefault="qualified" attributeFormDefault="unqualified" jxb:version="1.0">
   <xs:include schemaLocation="CPRulesCommonElementsSchema.xsd"/>
   <xs:element name="result">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="client" type="rf:clientType" minOccurs="0"/>
            <xs:element name="messages" type="rf:messageListType" minOccurs="0"/>
            <xs:element name="order_dataset" type="rf:datasetType" minOccurs="0"/>
            <xs:element name="discovered_dataset" type="rf:datasetType" minOccurs="0"/>
            <xs:element name="product_results" type="rf:productResultsType" minOccurs="0"/>
         </xs:sequence>
         <xs:attribute name="transaction_id" type="xs:string" use="optional"/>
         <xs:attribute name="result_id" type="xs:string" use="optional"/>
         <xs:attribute name="timestamp" type="xs:dateTime" use="required"/>
         <xs:attribute name="final" type="xs:boolean" use="required"/>
      </xs:complexType>
   </xs:element>	<!-- result -->

   <xs:complexType name="productResultsType">
      <xs:choice maxOccurs="1">
         <xs:element ref="rf:auto_data_prefill" />                
         <xs:element ref="rf:vehicle_mileage"/>
         <xs:element ref="rf:property_data_prefill" />
         <xs:element ref="rf:clue_commercial"/>
         <xs:element ref="rf:irs_verify"/>
      <xs:choice maxOccurs="unbounded">
         <xs:annotation>
            <xs:appinfo>
               <jxb:property name="productResultList"/>
            </xs:appinfo>
         </xs:annotation>
         <xs:element ref="rf:national_credit_file"/>
         <xs:element ref="rf:ssn_validation"/>
         <xs:element ref="rf:national_criminal"/>
         <xs:element ref="rf:clue_personal_auto"/>
         <xs:element ref="rf:clue_personal_property"/>
         <xs:element ref="rf:vehicle_identification"/>
         <xs:element ref="rf:autocheck_vehicle_identification"/>
         <xs:element ref="rf:motor_vehicle_report" />
         <xs:element ref="rf:commercial_national_credit_file" />
         <xs:element ref="rf:commercial_compid" />
         <xs:element ref="rf:commercial_scoring" />
         <xs:element ref="rf:carrier_discovery" />
		 <xs:element ref="rf:carrier_discovery_enhanced" />
         <xs:element ref="rf:claims_discovery" />
		 <xs:element ref="rf:carrier_id"/>
		 <xs:element ref="rf:supplementary_report"/>
		 <xs:element ref="rf:commercial_credit"/>
		 <xs:element ref="rf:riskview_attribute"/>
         <xs:element ref="rf:life_eir"/>
         <xs:element ref="rf:report_alerts"/>
 		 <xs:element ref="rf:loss_damage_waiver_score"/>
 		 <xs:element ref="rf:claims_datafill" />
         <xs:element ref="rf:score_result" />
		 <xs:element name="phone_result"  type="rf:phone_search" />
		 <xs:element name="rollup_person"  type="rf:rollup_person_search" />
		 <xs:element ref="rf:household_member"/>
         <xs:element ref="rf:instantid"/>
         <xs:element ref="rf:prescription_history"/>
		 <xs:element ref="rf:home_inspection_index"/>
		 <xs:element ref="rf:carrier_discovery_live"/>
     	<xs:element ref="rf:market_view_integrated"/>
		<xs:element ref="rf:lead_optimizer"/>
		<xs:element ref="rf:BuildingPermit"/>
		<xs:element ref="rf:PhysiciansInfo"/>
		<xs:element ref="rf:PropertyData"/>
		<xs:element ref="rf:ForeclosureVacancyReport"/>
        <xs:element ref="rf:optional_products"/>
        <xs:element ref="rf:CurrentCarrierOnly"/>
        <xs:element ref="rf:FraudPoint"/>
      </xs:choice>
      </xs:choice>
   </xs:complexType> <!-- productResultsType -->

   <xs:element name="national_credit_file">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="messages" type="rf:messageListType" minOccurs="0"/>
            <xs:element name="report" type="rf:ncf_report" minOccurs="0"/>
            <xs:element name="formatted_report" type="rf:formated_report" minOccurs="0"/>
         </xs:sequence>
         <xs:attribute name="primary_subject" type="xs:string" use="required"/>
         <xs:attribute name="joint_subject" type="xs:string" use="optional"/>
         <xs:attribute name="subject_id" type="xs:string" use="optional"/>
      </xs:complexType>
   </xs:element> <!-- national_credit_file -->


   <xs:complexType name="ncf_report">
      <xs:simpleContent>
         <xs:extension base="xs:string">
            <xs:attribute name="format" type="rf:ncf_format" use="required"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>  <!-- ncf_report -->


   <xs:simpleType name="ncf_format">
      <xs:annotation>
         <xs:appinfo>
            <jxb:typesafeEnumClass name="NcfReturnFormatEnum"/>
         </xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="EDITS 1"/>
         <xs:enumeration value="EDITS 2"/>
         <xs:enumeration value="Print Image"/>
         <xs:enumeration value="CP XML"/>
           <xs:enumeration value="XHTML"/>
      </xs:restriction>
   </xs:simpleType>  <!-- ncf_format -->

   <!--   End of Ncf Information -->
   <xs:element name="commercial_national_credit_file">
