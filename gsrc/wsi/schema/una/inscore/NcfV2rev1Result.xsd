		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="current"/>
			<xs:enumeration value="first_former"/>
			<xs:enumeration value="second_former"/>
                        <xs:enumeration value="unknown"/>
		</xs:restriction>
		<!-- global Simple Type -->
	</xs:simpleType>

	<xs:simpleType name="score_status">
		<xs:annotation>
			<xs:appinfo>
				<jxb:typesafeEnumClass name="scoreStatusEnum"/>
			</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Scored"/>
			<xs:enumeration value="No Score"/>
			<xs:enumeration value="No Hit"/>
			<xs:enumeration value="No Result"/>
			<xs:enumeration value="Error"/>
                        <xs:enumeration value="Unknown"/>
		</xs:restriction>
		<!-- global Simple Type -->
	</xs:simpleType>

