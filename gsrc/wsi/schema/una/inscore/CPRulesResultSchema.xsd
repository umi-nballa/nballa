      </xs:simpleContent>
   </xs:complexType>  <!-- commercial_ncf_report -->


   <xs:simpleType name="commercial_ncf_format">
      <xs:annotation>
         <xs:appinfo>
            <jxb:typesafeEnumClass name="CommercialNcfReturnFormatEnum"/>
         </xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="CPUV6"/>
      </xs:restriction>
   </xs:simpleType>  <!-- commercial_ncf_format -->

   <xs:simpleType name="commercial_ncf_bureau">
      <xs:annotation>
         <xs:appinfo>
            <jxb:typesafeEnumClass name="CommercialNcfBureauEnum"/>
         </xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Experian"/>
         <xs:enumeration value="Equifax"/>
         <xs:enumeration value="Dun&amp;Bradstreet"/>
      </xs:restriction>
   </xs:simpleType>  <!-- commercial_ncf_bureau -->

   <!--   End of Commercial Ncf Information -->
   
   <xs:element name="commercial_credit">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="report" type="rf:commercial_credit_report" minOccurs="0"/>
			</xs:sequence>
