<xs:schema targetNamespace="http://cp.com/rules/client" xmlns:rf="http://cp.com/rules/client" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" elementFormDefault="qualified" jxb:version="1.0">

   <xs:include schemaLocation="CPRulesCommonElementsSchema.xsd"/>
   <xs:element name="order">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="client" type="rf:clientType"/>
            <xs:element ref="rf:accounting" minOccurs="0"/>
            <xs:element ref="rf:rule_plan" minOccurs="0"/>
            <xs:element name="return_format" type="rf:result_return_format" minOccurs="0"/>
            <xs:element name="products" type="rf:productChoiceType" minOccurs="0"/>
            <xs:element name="dataset" type="rf:datasetType"/>
           
          </xs:sequence>
         <xs:attribute name="version" type="xs:string" use="optional"/>
         <xs:attribute name="always_return_xml" type="xs:boolean" use="optional"/>
         <!-- added for pdf or tiff format conversion -->
	      </xs:complexType>
   </xs:element> <!-- order -->

   <xs:element name="accounting">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:pnc" minOccurs="0"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element> <!-- accounting -->

   <xs:simpleType name="yesno">
      <xs:annotation>
         <xs:appinfo>
            <jxb:typesafeEnumClass name="YesNoEnum"/>
         </xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="yes"/>
         <xs:enumeration value="no"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:element name="pnc">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="account">
               <!--Account Number would be either 6 digit or -->
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:pattern value="\d{6}([0-9 A-Z]{3})?"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
            <xs:element name="special_billing_id" minOccurs="0">
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:maxLength value="15"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
   </xs:element> <!-- pnc -->


   <xs:element name="rule_plan">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
         </xs:sequence>
         <xs:attribute name="id" type="xs:positiveInteger" use="optional"/>
      </xs:complexType>
   </xs:element> <!-- rule_plan -->

   <xs:complexType name="productChoiceType">
      <xs:choice maxOccurs="1">
         <xs:element ref="rf:auto_dataprefill"/>
         <xs:element ref="rf:vehicle_mileage"/>
         <xs:element ref="rf:claims_discovery"/>
	     <xs:element ref="rf:home_inspection_index"/>
	     <xs:element ref="rf:market_view_integrated"/>
         <xs:element ref="rf:property_dataprefill"/>
         <xs:element ref="rf:mvr_enhance_clue"/>
         <xs:element ref="rf:carrier_id"/>
         <xs:element ref="rf:clue_commercial"/>
         <xs:element ref="rf:report_alerts"/>
         <xs:element ref="rf:claims_datafill"/>
         <xs:element ref="rf:irs_verify"/>
         <xs:element ref="rf:clue_with_iid"/>
         <xs:choice maxOccurs="unbounded">
            <xs:annotation>
               <xs:appinfo>
                  <jxb:property name="productList"/>
               </xs:appinfo>
            </xs:annotation>
            <xs:element ref="rf:national_credit_file"/>
            <xs:element ref="rf:ssn_validation"/>
            <xs:element ref="rf:national_criminal"/>
            <xs:element ref="rf:clue_property"/>
            <xs:element ref="rf:clue_auto"/>
            <xs:element ref="rf:vehicle_identification"/>
            <xs:element ref="rf:autocheck_vehicle_identification"/>
            <xs:element ref="rf:motor_vehicle_report"/>
            <xs:element ref="rf:commercial_compid"/>
            <xs:element ref="rf:carrier_discovery"/>
            <xs:element ref="rf:commercial_credit"/>
            <xs:element ref="rf:riskview_attributes"/>
            <xs:element ref="rf:life_eir"/>
            <xs:element ref="rf:dataprefill_life"/>
             <xs:element ref="rf:prescription_history"/>
            <xs:element ref="rf:instantid"/>
			<xs:element ref="rf:commercial_dataprefill"/>
			<xs:element ref="rf:carrier_discovery_live"/>
			<xs:element ref="rf:optional_products"/>
			<xs:element ref="rf:lead_optimizer"/>
			<xs:element ref="rf:BuildingPermit"/>
			<xs:element ref="rf:PropertyData"/>
			<xs:element ref="rf:ForeclosureVacancy"/>
			<xs:element ref="rf:CurrentCarrierOnly"/>
			<xs:element ref="rf:FraudPoint"/>
			<xs:element ref="rf:PhysiciansInfo"/>
         </xs:choice>
      </xs:choice>
   </xs:complexType> <!-- productChoiceType -->

   <xs:element name="national_credit_file">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="rf:pnc" minOccurs="0"/>
            <xs:element name="type" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="NcfTypeEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="Regular NCF transaction"/>
                     <xs:enumeration value="FSI (Financial Stress Indicator)"/>
                     <xs:enumeration value="Government"/>
                     <xs:enumeration value="NCF Report"/>
                     <xs:enumeration value="Commercial Insurance"/>
                     <xs:enumeration value="Attract with Claims"/>
                     <xs:enumeration value="Independent Agent"/>
                     <xs:enumeration value="Social Search"/>
                     <xs:enumeration value="Parallel Product Report"/>
                     <xs:enumeration value="Address Update"/>
                     <xs:enumeration value="Canadian Search"/>
                     <xs:enumeration value="Multi Product Switch"/>
                     <xs:enumeration value="Concurrent"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>

            <xs:element name="usage" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="NcfUsageEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="Claims"/>
                     <xs:enumeration value="Underwriting"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>



            <xs:element name="current_carrier" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="NcfBusinessEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="Auto"/>
                     <xs:enumeration value="Property"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
            <xs:element name="report_code" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:javaType name="int" parseMethod="javax.xml.bind.DatatypeConverter.parseInt" printMethod="javax.xml.bind.DatatypeConverter.printInt"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:nonNegativeInteger">
                     <xs:maxInclusive value="9999"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
            <xs:element name="addon_product" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="NcfAddonProductEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="Current Carrier"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
            <xs:choice minOccurs="0">
               <xs:element name="vendor" minOccurs="0">
                  <xs:simpleType>
                     <xs:annotation>
                        <xs:appinfo>
                           <jxb:typesafeEnumClass name="NcfVendorEnum"/>
                        </xs:appinfo>
                     </xs:annotation>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="Equifax"/>
                        <xs:enumeration value="Experian"/>
                        <xs:enumeration value="TransUnion"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:element>
               <xs:element name="bypass_duplicate_order_check" minOccurs="0">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="yes"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:element>
            </xs:choice>
            <xs:element name="format" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="NcfReturnFormatEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="EDITS 1"/>
                     <xs:enumeration value="EDITS 2"/>
                     <xs:enumeration value="Print Image"/>
                     <xs:enumeration value="CP XML"/>
                      <xs:enumeration value="XHTML"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
            <xs:element name="model_id" minOccurs="0" maxOccurs="unbounded">
               <xs:annotation>
                  <xs:appinfo>
                     <jxb:property name="modelIdList"/>
                  </xs:appinfo>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:maxLength value="2"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>

            <xs:element name="policy_type" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="NcfPolicyTypeEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="Property 1"/>
                     <xs:enumeration value="Property 2"/>
                     <xs:enumeration value="Property All Risk"/>
                     <xs:enumeration value="Property Renter"/>
                     <xs:enumeration value="Property Condo"/>
                     <xs:enumeration value="Property Dwelling"/>
                     <xs:enumeration value="Auto Non-standard"/>
                     <xs:enumeration value="Auto Prefered"/>
                     <xs:enumeration value="Auto Preferred above minimum limits"/>
                     <xs:enumeration value="Auto Standard"/>
                     <xs:enumeration value="Auto Standard above minimum limits"/>
                     <xs:enumeration value="No score ordered"/>
                     <xs:enumeration value="Attract Auto 3.0"/>
                     <xs:enumeration value="Attract Home 3.0"/>
                     <xs:enumeration value="Attract One 3.0"/>
                     <xs:enumeration value="Other"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
            <xs:element name="policy_rating_state" minOccurs="0">
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:maxLength value="2"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="primary_subject" type="xs:IDREF" use="optional"/>
         <xs:attribute name="joint_subject" type="xs:IDREF" use="optional"/>
      </xs:complexType>
   </xs:element> <!-- national_credit_file -->


   <xs:element name="ssn_validation">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
         </xs:sequence>
         <xs:attribute name="subject" type="xs:IDREF" use="optional"/>
      </xs:complexType>
   </xs:element> <!-- ssn_validation -->


   <xs:element name="national_criminal">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="search" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="NationalCriminalSearchEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="lfmNameHitSearch"/>
                     <xs:enumeration value="lfmNameMasterSearch"/>
                     <xs:enumeration value="caseNumberHitSearch"/>
                     <xs:enumeration value="caseNumberMasterSearch"/>
                     <xs:enumeration value="fullNameHitSearch"/>
                     <xs:enumeration value="fullNameMasterSearch"/>
                     <xs:enumeration value="supplierList"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="subject" type="xs:IDREF" use="optional"/>
      </xs:complexType>
   </xs:element> <!-- national_criminal -->

   <xs:element name="carrier_discovery">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="company_number" type="xs:string"/>
            <xs:choice maxOccurs="1">
               <xs:element name="vin_search">
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element name="vin" type="xs:string"/>
                        <xs:element name="enhance_subject" type="xs:IDREF" minOccurs="0"/>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
               <xs:element name="subject_search">
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element name="subject" type="xs:IDREF"/>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
            </xs:choice>
         </xs:sequence>
      </xs:complexType>
   </xs:element> <!-- carrier_discovery -->

   <xs:element name="clue_property">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="rf:pnc" minOccurs="0"/>
            <xs:element name="usage" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="CluePropertyUsageEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="Underwriting"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
            <xs:element name="reportType" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="CluePropertyReportTypeEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="C.L.U.E. Property and Real Property"/>
                     <xs:enumeration value="Real Property only"/>
                     <xs:enumeration value="C.L.U.E. Property only"/>
                     <xs:enumeration value="Current Carrier / C.L.U.E. Property and Real Property"/>
                     <xs:enumeration value="Current Carrier / C.L.U.E. Property Only"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
            <xs:element name="riskAddress" type="xs:IDREF" minOccurs="0"/>
            <xs:element name="mailingAddress" type="xs:IDREF" minOccurs="0"/>
            <xs:element name="formerAddress" type="xs:IDREF" minOccurs="0"/>
            <xs:element name="mortgage" type="xs:IDREF" minOccurs="0"/>
            <xs:element name="format" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="CluePropertyReturnFormatEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="EDITS 1"/>
                     <xs:enumeration value="EDITS 2"/>
                     <xs:enumeration value="Print Image"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
         </xs:sequence>
         <xs:attribute name="primary_subject" type="xs:IDREF" use="optional"/>
         <xs:attribute name="joint_subject" type="xs:IDREF" use="optional"/>
         <xs:attribute name="requestor_producer_name" type="xs:string" use="optional"/>
      </xs:complexType>
   </xs:element> <!-- clue_property -->

   <xs:element name="commercial_compid">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="riskAddress" type="xs:IDREF" minOccurs="0"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element> <!-- commercial compid -->

   <!-- clue auto  -->
   <xs:element name="clue_auto">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="rf:pnc" minOccurs="0"/>
            <xs:element name="usage" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="ClueAutoUsageEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="Personal"/>
                     <xs:enumeration value="Commercial"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>
            <xs:element name="type" minOccurs="0">
               <xs:complexType>
                  <xs:simpleContent>
                     <xs:extension base="xs:string">
                  <xs:attribute name="subjectSearch" type="rf:yesno" use="optional" default="no"/>
                        <xs:attribute name="vinSearch" type="rf:yesno" use="optional" default="no"/>
                        <xs:attribute name="add" type="rf:yesno" use="optional" default="no"/>
                        <xs:attribute name="driverDiscovery" type="rf:yesno" use="optional" default="no"/>
                  <xs:attribute name="currentCarrier" type="rf:yesno" use="optional" default="no"/>
                  <xs:attribute name="attachments" type="rf:yesno" use="optional" default="no"/>
                     </xs:extension>
                  </xs:simpleContent>
               </xs:complexType>
            </xs:element>
            <xs:element name="format" minOccurs="0">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:appinfo>
                        <jxb:typesafeEnumClass name="ClueAutoReturnFormatEnum"/>
                     </xs:appinfo>
                  </xs:annotation>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="EDITS 1"/>
                     <xs:enumeration value="EDITS 2"/>
                     <xs:enumeration value="Print Image"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>

            <xs:element name="units" type="rf:clueSubjectVinType" minOccurs="1"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element> <!-- clue_auto -->

   <xs:complexType name="clueSubjectVinType">
      <xs:choice maxOccurs="50">
         <xs:annotation>
            <xs:appinfo>
               <jxb:property name="clueSubjectVehicleList"/>
            </xs:appinfo>
         </xs:annotation>
         <xs:sequence>
            <xs:element name="subject" type="rf:identificationType" minOccurs="0"/>
            <xs:element name="vehicle" type="rf:identificationType" minOccurs="0"/>
         </xs:sequence>
      </xs:choice>
   </xs:complexType> <!-- clueSubjectVinType -->

   <xs:complexType name="identificationType">
      <xs:simpleContent>
         <xs:extension base="xs:string">
            <xs:attribute name="ref" type="xs:IDREF" use="required"/>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType> <!-- identificationType -->
   
   <xs:simpleType name="productReturnFormat">
      <xs:annotation>
         <xs:appinfo>
            <jxb:typesafeEnumClass name="productReturnFormatEnum"/>
         </xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="EDITS 1"/>
         <xs:enumeration value="EDITS 2"/>
         <xs:enumeration value="MVR 100 Byte - Century Compliant"/>
         <xs:enumeration value="Print Image"/>
         <xs:enumeration value="CP XML"/>
      </xs:restriction>
   </xs:simpleType>
      
   <xs:simpleType name="driverHistoryAge">
      <xs:annotation>
         <xs:appinfo>
            <jxb:typesafeEnumClass name="driverHistoryAgeEnum"/>
         </xs:appinfo>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="thirtyDays"/>
         <xs:enumeration value="sixtyDays"/>
         <xs:enumeration value="ninetyDays"/>
         <xs:enumeration value="oneHundredTwentyDays"/>
      </xs:restriction>
   </xs:simpleType>


   <xs:element name="vehicle_identification">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="rf:pnc" minOccurs="0"/>
            <xs:element name="vehicles" minOccurs="0">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="vehicle" type="rf:identificationType" maxOccurs="99"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element name="format" type="rf:productReturnFormat" minOccurs="1"/>
         </xs:sequence>
         <xs:attribute name="primary_subject" type="xs:IDREF" use="optional"/>
         <xs:attribute name="type" type="rf:vin_report_type" use="required"/>
   </xs:complexType>   <!-- vehicle_identification -->
   </xs:element>
   <xs:element name="auto_dataprefill">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="rf:pnc" minOccurs="0"/>
            <xs:element name="format" type="rf:productReturnFormat" minOccurs="0"/>
         </xs:sequence>
         <xs:attribute name="primary_subject" type="xs:IDREF" use="required"/>
   </xs:complexType>   <!-- auto_data_prefill -->
   </xs:element>
    <xs:element name="claims_datafill">
      <xs:complexType>
         <xs:sequence>
			<xs:element name="carrier_name" type="xs:string"/>
			<xs:element name="carrier_policy_number" type="xs:string"/>
			<xs:element name="claim_number" type="xs:string"/>
			<xs:element name="claim_state" type="xs:string"/>
			<xs:element name="participant_number" type="xs:string"/>
			<xs:element name="date_of_loss" type="rf:USDateType" />
			<xs:element name="participant_type" type="rf:participantType" minOccurs="0" />
			<xs:element name="participant_role" type="rf:participantRole" minOccurs="0" />
	       	<xs:element name="participant_carrier_name" type="xs:string" minOccurs="0" />
			<xs:element name="participant_policy_number" type="xs:string" minOccurs="0"/>
			<xs:element name="subject_vin_info" type="rf:ClaimsDatafillSubjectVinType" minOccurs="1"/>
      	</xs:sequence>
         
   </xs:complexType>   <!-- claims_datafill -->
    </xs:element>
    
    <xs:complexType name="ClaimsDatafillSubjectVinType">
      <xs:choice maxOccurs="1">
         <xs:annotation>
            <xs:appinfo>
               <jxb:property name="ClaimsDatafillSubjectVehicleList"/>
            </xs:appinfo>
         </xs:annotation>
         <xs:sequence>
            <xs:element name="subject" type="xs:IDREF" minOccurs="0"/>
            <xs:element name="vehicle" type="xs:IDREF" minOccurs="0"/>
         </xs:sequence>
      </xs:choice>
   </xs:complexType> <!-- ClaimsDatafillSubjectVinType -->
   <xs:element name="autocheck_vehicle_identification">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="rf:parameter" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="rf:pnc" minOccurs="0"/>
            <xs:element name="vehicleIdentification" type="xs:IDREF" maxOccurs="99"/>
            <xs:element name="riskAddress" type="xs:IDREF" minOccurs="0"/>
            <xs:element name="format" type="rf:productReturnFormat" minOccurs="0"/>
         </xs:sequence>
         <xs:attribute name="primary_subject" type="xs:IDREF" use="optional"/>
   </xs:complexType>   <!-- autocheck_vehicle_identification -->
   </xs:element>

   <xs:complexType name="stateSpecificMvrType">  <!-- motor_vehicle_report type -->
      <xs:choice maxOccurs="1">
			<xs:element name="AZ" >   <!-- Arizona -->
            <xs:complexType>
               <xs:choice maxOccurs="1">
                  <xs:element name="fiveYears" type="xs:boolean" default="false"/>
                  <xs:element name="thirtyNineMonths" type="xs:boolean" default="false"/>
               </xs:choice>
            </xs:complexType>
         </xs:element>
			<xs:element name="AR" >  <!-- Arkansas -->
            <xs:complexType>
               <xs:choice maxOccurs="1">
                  <xs:element name="standard" type="xs:boolean" default="false"/>
                  <xs:element name="commercial" type="xs:boolean" default="false"/>
               </xs:choice>
            </xs:complexType>
         </xs:element>
         <xs:element name="GA" >   <!-- Georgia -->
            <xs:complexType>
