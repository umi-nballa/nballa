    <category
      code="HO3"
      typelist="HOPolicyType_HOE"/>
    <category
      code="HO6"
      typelist="HOPolicyType_HOE"/>
    <category
      code="HOB_Ext"
      typelist="HOPolicyType_HOE"/>
    <category
      code="NV"
      typelist="Jurisdiction"/>
    <category
      code="AZ"
      typelist="Jurisdiction"/>
  </typecode>
  <typecode
    code="ACV"
    desc="Actual Cash Value"
    name="Actual Cash Value"
    priority="2">
    <category
      code="BusinessOwnersLine"
      typelist="PolicyLine"/>
    <category
      code="TX"
      typelist="Jurisdiction"/>
    <category
      code="TDP1_Ext"
      typelist="HOPolicyType_HOE"/>
    <category
      code="TDP2_Ext"
      typelist="HOPolicyType_HOE"/>
    <category
      code="HOA_Ext"
      typelist="HOPolicyType_HOE"/>
  </typecode>
  <typecode
    code="FuncValue"
    desc="Functional value"
    name="Functional value"
    priority="3">
    <category
      code="BusinessOwnersLine"
      typelist="PolicyLine"/>
  </typecode>
  <typecode
    code="AgreedAmt"
    desc="Agreed amount"
    name="Agreed amount"
    priority="4">
    <category
      code="BusinessOwnersLine"
      typelist="PolicyLine"/>
  </typecode>
  <typecode
    code="ActCost"
    desc="Actual cost"
    name="Actual cost"
    retired="true"/>
  <typefilter
    desc="Valuation Methods Buildings and Contents "
    name="BuildingContentsFilter">
    <include
      code="ReplCost"/>
    <include
      code="FuncValue"/>
    <include
      code="ACV"/>
  </typefilter>
  <typefilter
    desc="Valuation Methods for Dwellings"
    name="DwellingFilter">
    <include
      code="ReplCost"/>
    <include
      code="ACV"/>
  </typefilter>
  <typefilter
    desc="Valutation Methods for Contractors Equipment"
    name="ContractorsEquipmentFilter">
    <include
      code="ACV"/>
    <include
      code="ReplCost"/>
  </typefilter>
  <typecode
    code="ReplWRoof"
    desc="Replacement Cost with Roof Surfacing"
    name="Replacement Cost with Roof Surfacing"
    priority="5">
    <category
      code="TX"
      typelist="Jurisdiction"/>
    <category
      code="HOA_Ext"
      typelist="HOPolicyType_HOE"/>
    <category
      code="HOB_Ext"
      typelist="HOPolicyType_HOE"/>
  </typecode>
  <typefilter
    desc="Coverage A Filter"
    name="CovAFilter">
    <include
      code="ACV"/>
    <include
      code="ReplCost"/>
    <include
      code="ReplWRoof"/>
  </typefilter>
  <typefilter
    desc="Personal Property Filter"
    name="PersonalPropertyFilter">
    <include
      code="PersProp_ACV"/>
    <include
      code="PersProp_ReplCost"/>
