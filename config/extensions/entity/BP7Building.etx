    desc="Fire Department Within 5 Miles"
    name="FireDepartmentDistance_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Air Conditioner Caged"
    name="Caged_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Air Conditioning Type"
    name="Alarmed_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Air Conditioning Type"
    name="Monitored_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Year Built"
    name="YearBuilt_Ext"
    nullok="true"
    type="year"/>
  <column
    desc="No Of stories"
    name="NoOfStories_Ext"
    nullok="true"
    type="nonnegativeinteger"/>
  <typekey
    desc="bp7predominentoccupancytype"
    name="PredominentOccType_Ext"
    nullok="true"
    typelist="BP7PredominentOccType_Ext"/>
