<?xml version="1.0"?>
<Entity
  name="BP7Building">
  <Columns>
    <Column
      beanPath="BP7Building.Building.Description"
      name="description"/>
    <Column
      beanPath="BP7Building.Building.BuildingNum"
      name="number"/>
  </Columns>
  <DisplayName><![CDATA[var suffix = displaykey.Web.Policy.BP7.Building.Building
if(description != null){
  suffix = description
}
return number + ": " + suffix]]></DisplayName>
</Entity>