                    target="bceg_Ext"/>
                </BooleanRadioInput>
                <Input
                  available="cpBuilding.OverrideBCEG_Ext"
                  editable="true"
                  id="BCEGUserEntered_Ext"
                  label="displaykey.Web.CP.CPLocation.BCEGUserEntered_Ext"
                  required="cpBuilding.OverrideBCEG_Ext"
                  value="cpBuilding.BCEGOverridden_Ext"
                  visible="cpBuilding.OverrideBCEG_Ext"/>
              </InputSet>
              <InputDivider/>
              <InputSet
                id="PCCode_Ext">
                <RangeInput
                  available="!cpBuilding.OverrideFirePCCode_Ext"
                  editable="true"
                  id="PCCodeTunaReturned_Ext"
                  label="displaykey.Web.CP.CPLocation.ProtectionClass"
                  value="cpBuilding.FireProtectionClassCode"
