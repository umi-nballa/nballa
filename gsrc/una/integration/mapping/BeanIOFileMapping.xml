            <field name="policyNumber" trim="true"/>
            <field name="policyTerm" trim="true"/>
            <field name="notes" trim="true"/>
        </record>
    </stream>

    <!-- Error Records file layout configuration : Used for all inbound integrations -->
    <stream name="FailedRecordsFileMapping" format="fixedlength">
        <record name="FailedRecord" class="map">
            <field name="recordText" type="string" length="unbounded"/>
