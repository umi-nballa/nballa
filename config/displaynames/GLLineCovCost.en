<?xml version="1.0"?>
<Entity
  name="GLLineCovCost">
  <Columns>
    <Column
      beanPath="GLLineCovCost.GeneralLiabilityCov"
      name="pattern"/>
    <Column
      beanPath="GLLineCovCost.GLCostType"
      name="costType"/>
  </Columns>
  <DisplayName><![CDATA[if(costType != null){
  return pattern.Pattern + " : " + costType.DisplayName  
}
return pattern.Pattern
  ]]></DisplayName>
</Entity>