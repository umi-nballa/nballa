<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Valuation Method"
  name="CPValuationMethod">
  <typecode
    code="ReplaceCost"
    desc="Replacement Cost"
    name="Replacement Cost"
    priority="1"/>
  <typecode
    code="FuncValue"
    desc="Functional Value"
    name="Functional Value"
    priority="2"/>
  <typecode
    code="ActualCash"
    desc="Actual Cash Value"
    name="Actual Cash Value"
    priority="3"/>
  <typecode
    code="AgreedAmt"
    desc="Agreed Amount"
    name="Agreed Amount"
    priority="4"
    retired="true"/>
  <typefilter
    desc="CP Business Personal Property Cov Filter"
    name="CPBPPCovValuationMethodFilter">
    <include
      code="ReplaceCost"/>
  </typefilter>
  <typefilter
    desc="CP Stock Cov Filter"
    name="CPStockCovValuationMethodFilter">
    <include
      code="ReplaceCost"/>
    <include
      code="ActualCash"/>
  </typefilter>
  <typefilter
    desc="Building Valuation Method"
    name="CPBldgValuation_EXT">
    <include
      code="AgreedAmt"/>
    <include
      code="ReplaceCost"/>
    <include
      code="AgreedVal"/>
  </typefilter>
  <typecode
    code="AgreedVal"
    desc="Agreed Value"
    name="Agreed Value"
    priority="5"/>
</typelistextension>