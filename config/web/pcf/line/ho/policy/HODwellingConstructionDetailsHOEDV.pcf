          editable="!dwelling.OverrideStoriesNumber_Ext"
          id="NumberOfStoriesTunaReturned_Ext"
          label="displaykey.Web.Homeowners.HOLocation.NumberOfStories_Ext"
          value="dwelling.StoriesNumber"
          valueRange="gw.lob.ho.HODwellingUtil_HOE.getTunaCodes(tunaAppResponse.StoryNumber)">
          <PostOnChange/>
        </RangeInput>
        <Input
          id="NumberOfStoriesMatchLevel_Ext"
          label="displaykey.Web.Homeowners.HOLocation.NumberOfStoriesMatchLevel_Ext"
          value="dwelling.StoriesNumberMatchLevel_Ext"/>
        <BooleanRadioInput
          editable="true"
          id="OverRideNumberOfStories_Ext"
          label="displaykey.Web.Homeowners.HOLocation.NumberOfStoriesOverride_Ext"
          value="dwelling.OverrideStoriesNumber_Ext">
          <PostOnChange
            target="NumberOfStories_Ext"/>
        </BooleanRadioInput>
        <Input
          available="dwelling.OverrideStoriesNumber_Ext"
          editable="true"
          id="NumberOfStoriesUserEntered_Ext"
          label="displaykey.Web.Homeowners.HOLocation.NumberOfStoriesUserEntered_Ext"
          required="dwelling.OverrideStoriesNumber_Ext"
          value="dwelling.NoOfStoriesOverridden_Ext"
          visible="dwelling.OverrideStoriesNumber_Ext"/>
