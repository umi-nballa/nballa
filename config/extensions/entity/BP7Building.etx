  <column
    columnName="bp7sprinklered"
    default="false"
    desc="Automatic Sprinkler System"
    name="Sprinklered"
    nullok="false"
    type="bit"/>
  <typekey
    columnName="bp7pctowneroccupied"
    default=""
    desc="Percentage Owner Occupied"
    name="PctOwnerOccupied"
    nullok="true"
    typelist="BP7PctOwnerOccupied"/>
