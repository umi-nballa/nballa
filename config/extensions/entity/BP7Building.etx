    desc="Central Burglar Alarm"
    name="CentralBurglarAlarm_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Central Fire Alarm System"
    name="CentralFireAlarmSystem_Ext"
    nullok="true"
    type="bit"/>
