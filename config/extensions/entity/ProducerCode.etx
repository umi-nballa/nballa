    fkentity="Contact"
    name="Contact_Ext"
    nullok="true"
    owner="true"/>
  <column
    desc="Direct Deposit Indicator"
    name="UseDirectDeposit_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Print Information on Dec"
    name="PrintOnDec_Ext"
    nullok="true"
    type="bit"/>
  <column
