<?xml version="1.0"?>
<!-- Typelist for NotificationConfig.ActionType column -->
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Notification configuration action types"
  name="NotificationActionType">
  <typecode
    code="UWPeriodHO"
    desc="Underwriting free period specific to homeowners policy types  in days"
    name="Underwriting Free Period Homeowners-Specific">
    <category
      code="uwcancel"
      typelist="NotificationCategory"/>
  </typecode>
  <typecode
    code="UWPeriodDP"
    desc="Underwriting free period specific to fire dwelling policy types  in days"
    name="Underwriting Free Period Fire Dwelling-Specific">
    <category
      code="uwcancel"
      typelist="NotificationCategory"/>
  </typecode>
  <typecode
    code="UWCancelInferred"
    desc="An inferred UW Cancel action type.  Used for when a notification time is needed for special cases"
    name="UW Cancel Inferred"/>
</typelistextension>