    <xs:simpleType name="narrativeType">
        <xs:annotation>
            <xs:appinfo>
                <jxb:typesafeEnumClass name="narrativeTypesEnum"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="city"/>
            <xs:enumeration value="state"/>
            <xs:enumeration value="unknown"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="narratives">
        <xs:sequence>
            <xs:element name="narrative" minOccurs="0" maxOccurs="4">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="type" type="rf:narrativeType"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="ncf_report">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="rf:admin"/>
                <xs:element ref="rf:report"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- root element requence -->
    <xs:element name="report">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="rf:alerts_scoring" minOccurs="0" />
