    desc="Wind Pool Eligibility returned by Tuna"
    name="WindPool_Ext"
    nullok="true"
    type="bit"/>
  <typekey
    desc="Wind Pool Eligible Match Level returned by TUNA"
    name="WindPoolMatchLevel_Ext"
    nullok="true"
    typelist="TUNAMatchLevel_Ext"/>
  <column
    default="0"
    desc="Has the Wind Pool Value returned from TUNA Overrriden"
    name="OverrideWindPool_Ext"
    nullok="true"
    type="bit"/>
  <column
    default="0"
    desc="Wind Pool Eligible Value Overridden by User"
    name="WindPoolOverridden_Ext"
    nullok="true"
    type="bit"/>
