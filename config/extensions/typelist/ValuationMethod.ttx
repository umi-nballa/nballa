<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Types of valuation methods"
  name="ValuationMethod">
  <typecode
    code="Appraisal"
    desc="Appraisal"
    name="Appraisal"
    retired="true"/>
  <typecode
    code="CompSale"
    desc="Comparable sale"
    name="Comparable sale"
    retired="true"/>
  <typecode
    code="SaleRec"
    desc="Sales receipt"
    name="Sales receipt"
    retired="true"/>
  <typecode
    code="ReplCost"
    desc="Replacement Cost"
    name="Replacement Cost"
    priority="1">
    <category
      code="BusinessOwnersLine"
      typelist="PolicyLine"/>
    <category
      code="CA"
      typelist="Jurisdiction"/>
    <category
      code="FL"
      typelist="Jurisdiction"/>
    <category
      code="HI"
      typelist="Jurisdiction"/>
    <category
      code="NC"
      typelist="Jurisdiction"/>
    <category
      code="TX"
      typelist="Jurisdiction"/>
    <category
      code="TDP3_Ext"
      typelist="HOPolicyType_HOE"/>
    <category
      code="DP3_Ext"
