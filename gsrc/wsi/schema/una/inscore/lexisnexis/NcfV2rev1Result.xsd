                <xs:element ref="rf:consumer_statements" minOccurs="0"/>
                <xs:element ref="rf:inquiry_history" minOccurs="0"/>
                <xs:element ref="rf:additional_information" minOccurs="0"/>
                <xs:element ref="rf:lexisnexis_lien_judgment" minOccurs="0"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:simpleType name="ljStatusEnum">
        <xs:annotation>
            <xs:appinfo>
                <jxb:typesafeEnumClass name="ljStatusEnum" />
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Insufficient Data - could not locate consumer" />	<!-- 1 -->
            <xs:enumeration value="No Hit" />											<!-- 2 -->
            <xs:enumeration value="Product unavailable" />								<!-- 3 -->
            <xs:enumeration value="Complete" />											<!-- 4 -->
            <xs:enumeration value="L and J Not Requested" />							<!-- blank -->
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ljSourceIndicatorEnum">
        <xs:annotation>
            <xs:appinfo>
                <jxb:typesafeEnumClass name="ljSourceIndicatorEnum" />
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="LexisNexis" /> 				<!-- 1,2,3,4 -->
            <xs:enumeration value="Vendor" />					<!-- blank -->
        </xs:restriction>
    </xs:simpleType>

