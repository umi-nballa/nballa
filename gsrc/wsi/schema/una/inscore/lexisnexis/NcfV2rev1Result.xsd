            <xs:enumeration value="In WEP of other party"/>
            <xs:enumeration value="Adjustment pending"/>
            <xs:enumeration value="Escrow account only"/>
            <xs:enumeration value="BMO need authorization to clear"/>
            <xs:enumeration value="Account included in CH13"/>
            <xs:enumeration value="Creditor declines to clear"/>
            <xs:enumeration value="No response from creditor"/>
            <xs:enumeration value="Dispute - resolution pending"/>
            <xs:enumeration value="Out-of-area trade"/>
            <xs:enumeration value="Unable to check without account number"/>
