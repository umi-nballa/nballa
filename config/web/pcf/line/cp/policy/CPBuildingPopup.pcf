              </InputGroup>
              <InputDivider/>
              <InputSet
                id="Windpoolvalue_Ext">
                <RangeInput
                  available="!cpBuilding.OverrideWindPool_Ext"
                  editable="true"
                  id="WindpoolvalueTunaReturned_Ext"
                  label="displaykey.Web.CP.CPLocation.WindpoolvalueTunaReturned_Ext"
                  value="cpBuilding.Windpoolvalue_Ext"
                  valueRange="gw.lob.ho.HODwellingUtil_HOE.getTunaCodes(tunaAppResponse.WindPool)"/>
                <Input
                  available="!cpBuilding.OverrideWindPool_Ext"
                  id="WindpoolvalueMatchLevel_Ext"
                  label="displaykey.Web.CP.CPLocation.WindpoolvalueMatchlevel_Ext"
                  value="cpBuilding.WindPoolMatchLevel_Ext"/>
                <BooleanRadioInput
                  editable="true"
                  id="OverRideWindpoolvalue_Ext"
                  label="displaykey.Web.CP.CPLocation.WindpoolvalueOverride_Ext"
                  value="cpBuilding.OverrideWindPool_Ext">
                  <PostOnChange
                    target="Windpoolvalue_Ext"/>
                </BooleanRadioInput>
                <Input
                  available="cpBuilding.OverrideWindPool_Ext"
                  editable="true"
                  id="WindpoolvalueUserEntered_Ext"
                  label="displaykey.Web.CP.CPLocation.WindpoolvalueUserEntered_Ext"
                  required="cpBuilding.OverrideWindPool_Ext"
                  value="cpBuilding.WindPoolOverridden_Ext"
                  visible="cpBuilding.OverrideWindPool_Ext"/>
              </InputSet>
              <InputDivider/>
              <InputDivider/>
              <InputSet
                id="bceg_Ext">
                <RangeInput
                  editable="!cpBuilding.OverrideBCEG_Ext"
                  id="BCEGTunaReturned_Ext"
                  label="displaykey.Web.CP.CPLocation.BCEGTunaReturned_Ext"
                  value="cpBuilding.BCEG_Ext"
                  valueRange="gw.lob.ho.HODwellingUtil_HOE.getTunaCodes(tunaAppResponse.BCEGGrade)"/>
                <Input
                  id="BCEGMatchLevel_Ext"
                  label="displaykey.Web.CP.CPLocation.BCEGMatchlevel_Ext"
