<?xml version="1.0"?>
<Entity
  name="BP7Classification">
  <Columns>
    <Column
      beanPath="BP7Classification.ClassDescription.DisplayName"
      name="description"/>
    <Column
      beanPath="BP7Classification.ClassificationNumber"
      name="number"/>
  </Columns>
  <DisplayName><![CDATA[var suffix = displaykey.Web.Policy.BP7.Classification.Classification
if(description != null){
  suffix = description
}
return number + ": " + suffix]]></DisplayName>
</Entity>