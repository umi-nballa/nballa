<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <Page
    afterEnter="gw.api.web.PebblesUtil.addWebMessages(CurrentLocation, policyPeriod.PolicyFileMessages)"
    canVisit="perm.PolicyPeriod.view(policyPeriod) and perm.System.pfiledetails"
    id="PolicyFile_BP7Location"
    parent="PolicyFile(policyPeriod, asOfDate)"
    title="displaykey.Web.Policy.BP7.Location.NamePlural">
    <LocationEntryPoint
      signature="PolicyFile_BP7Location(policyPeriod : PolicyPeriod, asOfDate : DateTime)"/>
    <Variable
      name="policyPeriod"
      type="PolicyPeriod"/>
    <Variable
      name="asOfDate"
      type="DateTime"/>
    <Screen
      id="PolicyFile_BP7LocationScreen">
      <TitleBar/>
      <Toolbar/>
      <PanelRef
        def="BP7LocationPanelSet(policyPeriod, false, null)"/>
    </Screen>
  </Page>
</PCF>