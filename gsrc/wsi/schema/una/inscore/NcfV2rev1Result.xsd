	<!-- public_records -->
	<xs:element name="admin">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="quoteback" maxOccurs="unbounded">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:string">
								<xs:attribute name="name" type="xs:string" use="required"/>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="product_group">
					<xs:simpleType>
						<xs:annotation>
							<xs:appinfo>
								<jxb:typesafeEnumClass name="productGroupTypeEnum"/>
							</xs:appinfo>
						</xs:annotation>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Regular NCF transaction"/>
							<xs:enumeration value="FSI (Financial Stress Indicator)"/>
							<xs:enumeration value="Government"/>
							<xs:enumeration value="NCF Report"/>
							<xs:enumeration value="Commercial Insurance"/>
							<xs:enumeration value="Attract with Claims"/>
							<xs:enumeration value="Independent Agent"/>
							<xs:enumeration value="Social Search"/>
							<xs:enumeration value="Parallel Product Report"/>
							<xs:enumeration value="Address Update"/>
							<xs:enumeration value="Canadian Search"/>
							<xs:enumeration value="Comparative Credit"/>
							<xs:enumeration value="NCF transaction with Current Carrier"/>
							<xs:enumeration value="Canadian report"/>
                            <xs:enumeration value="Unknown"/>
						</xs:restriction>
					</xs:simpleType>
					<!-- RI51 Product Group -->
				</xs:element>
				<xs:element name="pnc_account">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="\d{6}[A-Z]{3}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="pnc_account_name" type="xs:string" minOccurs="0"/>
				<xs:element name="product_reference" type="xs:string" minOccurs="0"/>
				<xs:element name="report_type">
					<xs:simpleType>
						<xs:annotation>
							<xs:appinfo>
								<jxb:typesafeEnumClass name="reportTypeEnum"/>
							</xs:appinfo>
						</xs:annotation>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Individual"/>
							<xs:enumeration value="Joint"/>
				                        <xs:enumeration value="Unknown"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="status">
					<xs:simpleType>
						<xs:annotation>
							<xs:appinfo>
								<jxb:typesafeEnumClass name="processingStatusEnum"/>
							</xs:appinfo>
						</xs:annotation>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Complete, No Report (credit bureau manual report)"/>
							<xs:enumeration value="Credit bureau no-hit"/>
							<xs:enumeration value="Credit bureau designed error"/>
							<xs:enumeration value="Invalid Account"/>
							<xs:enumeration value="Invalid Request"/>
							<xs:enumeration value="Complete"/>
							<xs:enumeration value="Unscored"/>
							<xs:enumeration value="PLS >= account threshold"/>
							<xs:enumeration value="Low PLS"/>
			                                <xs:enumeration value="Unknown"/>
						</xs:restriction>
					</xs:simpleType>
					<!-- RI51 Processing Status -->
				</xs:element>
				<xs:element name="ownership" minOccurs="0">
					<xs:simpleType>
						<xs:annotation>
							<xs:appinfo>
								<jxb:typesafeEnumClass name="ownershipEnum"/>
							</xs:appinfo>
						</xs:annotation>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Personal"/>
							<xs:enumeration value="Commercial"/>
				                        <xs:enumeration value="Unknown"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="special_billing_id" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="report_code" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:nonNegativeInteger">
							<xs:maxInclusive value="9999"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="report_description" type="xs:string" minOccurs="0"/>
				<xs:element name="line_of_business" minOccurs="0">
					<xs:simpleType>
						<xs:annotation>
							<xs:appinfo>
								<jxb:typesafeEnumClass name="lineOfBusinessEnum"/>
							</xs:appinfo>
						</xs:annotation>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Property"/>
							<xs:enumeration value="Auto"/>	
							<xs:enumeration value="Financial"/>
				                        <xs:enumeration value="Unknown"/>
						</xs:restriction>
					</xs:simpleType>
					<!-- RI51 -->
				</xs:element>
				<xs:element name="purpose" minOccurs="0">
					<xs:simpleType>
						<xs:annotation>
							<xs:appinfo>
								<jxb:typesafeEnumClass name="purposeEnum"/>
							</xs:appinfo>
						</xs:annotation>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Underwriting"/>
							<xs:enumeration value="Claims"/>
							<xs:enumeration value="Financial"/>
				                        <xs:enumeration value="Unknown"/>
						</xs:restriction>
					</xs:simpleType>
					<!-- RI51 -->
				</xs:element>
				<xs:element name="date_request_ordered" type="rf:USDateType" minOccurs="0"/>
				<xs:element name="date_request_received" type="rf:USDateType" minOccurs="0"/>
				<xs:element name="date_request_completed" type="rf:USDateType" minOccurs="0"/>
				<xs:element name="time_report_processed" type="rf:edits_time" minOccurs="0"/>
				<xs:element name="multiple_scores_ordered" type="rf:true_false" minOccurs="0"/>
				<xs:element name="vendor" minOccurs="0">
					<xs:complexType>
						<xs:attribute name="name" type="rf:credit_vendors" /> <!-- RI52 Special Field #3 -->
						<xs:attribute name="address" type="xs:string" />
						<xs:attribute name="status">
							<xs:simpleType>
								<xs:annotation>
									<xs:appinfo>
										<jxb:typesafeEnumClass name="vendorStatusEnum"/>
									</xs:appinfo>
								</xs:annotation>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Equifax No Hit"/>
									<xs:enumeration value="Equifax System Unavailable"/>
									<xs:enumeration value="TransUnion No Hit"/>
									<xs:enumeration value="TransUnion System Unavailable"/>
									<xs:enumeration value="Experian No Hit"/>
									<xs:enumeration value="Experian System Unavailable"/>
									<xs:enumeration value="Completed"/>
									<xs:enumeration value="Unknown"/>
								</xs:restriction>
							</xs:simpleType>
									<!-- RI52 Special Field #3 -->
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="report"  minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="sequence" type="xs:string" minOccurs="0"/>
							<xs:element name="count" type="xs:string" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>	
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- base elements of 1 admin section -->
	<xs:element name="alerts_scoring">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="scoring" minOccurs="0" maxOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="score" minOccurs="1" maxOccurs="9" >
								<xs:complexType>
									<xs:sequence>
										<xs:element name="model_label" type="xs:string" minOccurs="0" />
										<xs:element name="score" type="rf:counter" minOccurs="0" />
										<xs:element name="rating_state" type="xs:string" minOccurs="0" />
										<xs:element name="model_id" type="xs:string" minOccurs="0" />
										<xs:element name="classification" type="rf:AlertsClassification" minOccurs="0"/>
										<xs:element name="exclusion_code" type="xs:string" minOccurs="0"  />
										<xs:element name="exclusion_msg" type="xs:string" minOccurs="0" />
										<xs:element name="reason_codes" minOccurs="0" maxOccurs="99" >
											<xs:complexType>
												<xs:sequence>
													<xs:element name="code" type="xs:string" minOccurs="0" />
													<xs:element name="description" type="xs:string" minOccurs="0" />
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="status" type="rf:score_status" />
								</xs:complexType>
							</xs:element>
						</xs:sequence>
