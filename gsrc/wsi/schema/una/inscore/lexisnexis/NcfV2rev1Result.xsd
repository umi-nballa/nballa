            <xs:enumeration value="No Hit"/>
            <xs:enumeration value="No Result"/>
            <xs:enumeration value="Error"/>
            <xs:enumeration value="Unknown"/>
        </xs:restriction>
        <!-- global Simple Type -->
    </xs:simpleType>

