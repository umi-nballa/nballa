    desc="Dwelling Protection Class lookup on address field and Distance to Fire Station based on ISO classification manual"
    name="DwellingProtectionClassCode"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="100"/>
  </column>
  <column
    desc="Dwelling Protection Class Code Overridden"
    name="DwellingPCCodeOverridden_Ext"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="100"/>
  </column>
  <typekey
    desc="Dwelling Protection Class Code Match Level from TUNA"
    name="DwellingPCCodeMatchLevel_Ext"
    nullok="true"
    typelist="TUNAMatchLevel_Ext"/>
  <column
    default="0"
    desc="Override Dwelling PC Code returned from TUNA"
    name="OverrideDwellingPCCode_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Responding Fire Dept Returned by Tuna"
    name="ResFireDept_Ext"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="100"/>
  </column>
  <typekey
    desc="Responding Fire Dept Match Level from TUNA"
    name="ResFireDeptMatchLevel_Ext"
    nullok="true"
    typelist="TUNAMatchLevel_Ext"/>
  <column
    default="0"
    desc="Has responding fire dept returned by Tuna Overridden"
    name="OverrideResFireDept_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Responding Fire Dept Overridden by User"
    name="ResFireDeptOverridden_Ext"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="100"/>
  </column>
  <typekey
    desc="Distance to Coast entered by Users"
    name="DistToCoastOverridden_Ext"
    nullok="true"
    typelist="DistToCoastOverridden_Ext"/>
  <column
    name="DistanceToCoast_Ext"
    nullok="true"
    type="positiveinteger"/>
  <typekey
    desc="Distance to Coast Returned by Tuna"
    name="DistToCoast_Ext"
    nullok="true"
    typelist="DistToCoastOverridden_Ext"/>
  <typekey
    desc="Distance To Coast Match Level from TUNA"
    name="DistToCoastMatchLevel_Ext"
    nullok="true"
    typelist="TUNAMatchLevel_Ext"/>
  <column
    default="0"
    desc="Override Wind Pool Value returned from TUNA"
    name="OverrideDistToCoast_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Fire Hydrant located within 1000 feet"
    name="FireHydrantDistance_Ext"
    nullok="true"
    type="bit"/>
  <column
    desc="Date of Heating Upgrade"
    name="HeatingUpgradeDate_Ext"
    nullok="true"
    type="year"/>
  <column
    desc="Date of Plumbing Upgrade"
    name="PlumbingUpgradeDate_Ext"
    nullok="true"
    type="year"/>
  <column
    desc="Date of Electrical System Upgrade"
    name="ElectricalSysUpgradeDate_Ext"
    nullok="true"
    type="year"/>
  <column
    desc="Date of Roofing Upgrade"
    name="RoofingUpgradeDate_Ext"
    nullok="true"
    type="year"/>
</extension>